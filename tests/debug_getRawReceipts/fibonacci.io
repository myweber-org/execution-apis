    (fibonacci(i)) println
)
fib := method(n,
    cache := Map clone
    cache atPut(0, 0)
    cache atPut(1, 1)
    
    fibMemo := method(n,
        if(cache hasKey(n),
            cache at(n),
            result := fibMemo(n-1) + fibMemo(n-2)
            cache atPut(n, result)
            result
        )
    )
    
    fibMemo(n)
)


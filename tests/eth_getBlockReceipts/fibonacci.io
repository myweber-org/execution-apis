
fib := method(n,
    memo := Map clone
    memo atPut(0, 0)
    memo atPut(1, 1)
    
    fibRec := method(k,
        memo hasKey(k) ifTrue(
            return memo at(k)
        )
        result := fibRec(k-1) + fibRec(k-2)
        memo atPut(k, result)
        result
    )
    
    fibRec(n)
)

0 to(10) foreach(i,
    fib(i) println
)